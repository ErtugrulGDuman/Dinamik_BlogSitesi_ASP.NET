@model IEnumerable<AltTab.Models.Category>


<div class="list-group">
    <a href="/Home/Index" class="list-group-item list-group-item-action active" aria-current="true">
        Kategoriler
    </a>
    @foreach(var item in Model)
            {
    <a href="/Home/MakaleListesi/@item.CategoryId" class="list-group-item list-group-item-action">@item.CategoryName</a>
            }
</div>
